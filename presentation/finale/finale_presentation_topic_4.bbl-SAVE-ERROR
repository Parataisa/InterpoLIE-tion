      \strng{namehash}{38b93968f6decc3b8e61769a499dcfca}
      \strng{fullhash}{38b93968f6decc3b8e61769a499dcfca}
      \strng{fullhashraw}{38b93968f6decc3b8e61769a499dcfca}
      \strng{bibnamehash}{38b93968f6decc3b8e61769a499dcfca}
